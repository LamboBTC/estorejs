{
  "name": "seller",
  "version": "0.1.0",
  "description": "Default theme for estore.",
  "author": "Lasana Murray <lmurray@quenk.com> (http://blog.quenk.com/)",
  "license": "Apache2",
  "estore": {
    "serve": {
      "static": {
        "/": "index/index.html",
        "/cart": "cart/index.html",
        "/checkout": "checkout/index.html",
        "/store/categories": "store/category.html"
      },
      "pattern": {
        "^/store/categories/([-\\w]{1,128})$": "store/category.html",
        "^/store/products/([-\\w]{1,128})$": "store/product.html"
      }
    }
  },
  "devDependencies": {
    "angular": "^1.2.19",
    "angular-ui-router": "^0.2.10",
    "bignumber.js": "^1.4.1",
    "browserify": "^4.1.5",
    "grunt": "^0.4.5",
    "grunt-angular-templates": "^0.5.5",
    "grunt-browserify": "^2.1.0",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-html2js": "^0.2.8",
    "gulp": "^3.6.2",
    "html2js": "0.0.3",
    "load-grunt-tasks": "^0.6.0",
    "minifyify": "^3.0.10",
    "partialify": "^3.1.1",
    "time-grunt": "^0.3.2",
    "uglifyify": "^2.4.0",
    "uglifyjs": "^2.3.6"
  },
  "dependencies": {
    "angular-spinner": "^0.5.0",
    "bignumber.js": "^1.4.0",
    "grunt-debug-task": "^0.1.4"
  }
}
